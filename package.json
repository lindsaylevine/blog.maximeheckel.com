{
  "name": "blog.maximeheckel.com",
  "private": true,
  "description": "blog.maximeheckel.com - Blog",
  "version": "0.5.0",
  "author": "Maxime Heckel <hello@maximeheckel.com>",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-on-netlify",
    "start": "next start",
    "export": "next export",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/css": "11.1.3",
    "@emotion/react": "11.1.4",
    "@emotion/server": "11.0.0",
    "@emotion/styled": "11.0.0",
    "@sindresorhus/slugify": "^0.11.0",
    "critters": "0.0.6",
    "focus-trap-react": "8.3.2",
    "framer-motion": "3.1.1",
    "globby": "^11.0.2",
    "graphql": "15.4.0",
    "graphql-request": "3.3.0",
    "gray-matter": "4.0.2",
    "mousetrap": "1.6.5",
    "next": "10.0.4",
    "next-mdx-remote": "2.0.1",
    "next-on-netlify": "2.7.0",
    "next-seo": "4.17.0",
    "playwright": "^1.7.1",
    "playwright-aws-lambda": "^0.6.0",
    "playwright-core": "^1.7.1",
    "plyr": "^3.6.3",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.23.0",
    "qs": "^6.9.4",
    "react": "17.0.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "17.0.0",
    "react-intersection-observer": "^8.27.1",
    "react-live": "2.2.3",
    "react-query": "2.25.2",
    "react-scrollspy": "3.4.3",
    "reading-time": "^1.2.1",
    "recharts": "1.8.5",
    "remark-autolink-headings": "^6.0.1",
    "remark-slug": "^6.0.0",
    "rss": "1.2.2",
    "unist-util-find-after": "^3.0.0",
    "unist-util-visit": "^2.0.3",
    "unist-util-visit-parents": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@cypress/snapshot": "^2.1.7",
    "@emotion/babel-preset-css-prop": "11.0.0",
    "@emotion/jest": "11.1.0",
    "@next/bundle-analyzer": "^10.0.5",
    "@now/node": "1.8.5",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^10.0.1",
    "@types/jest": "26.0.19",
    "@types/mousetrap": "^1.6.5",
    "@types/node": "14.14.20",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-scrollspy": "3.3.3",
    "@types/recharts": "^1.8.16",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-jest": "26.6.3",
    "cross-env": "^7.0.3",
    "cypress": "5.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^1.1.1",
    "jest": "26.6.3",
    "mdx-embed": "^0.0.17",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.11",
    "typescript": "4.1.3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaximeHeckel/blog.maximeheckel.com"
  },
  "resolutions": {
    "react": "17.0.0",
    "react-dom": "17.0.0"
  },
  "jest": {
    "snapshotSerializers": [
      "@emotion/jest"
    ],
    "setupFilesAfterEnv": [
      "./setup-jest.js"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.[jt]sx?$": "<rootDir>/jest-preprocess.js"
    },
    "transformIgnorePatterns": [],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
